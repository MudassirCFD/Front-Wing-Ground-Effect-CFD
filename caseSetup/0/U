FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (50 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (50 0 0);
    }

    outlet
    {
        type            zeroGradient;
    }

    bottom
    {
        // Moving ground (same speed as freestream, opposite sign would be wrong here)
        type            movingWallVelocity;
        value           uniform (50 0 0);
    }

    top
    {
        type            symmetryPlane;
    }
    left_side
    {
        type            symmetryPlane;
    }
    right_side
    {
        type            symmetryPlane;
    }

    wing
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    // If you also have wing_Front_Wing, copy same block for it.
}
